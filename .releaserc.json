{
  "branches": [
    "main",
    {
      "name": "feature/*",
      "prerelease": "feature"
    },
    {
      "name": "bug-fix/*",
      "prerelease": "bugfix"
    },
    {
      "name": "release/*",
      "prerelease": "release"
    },
    {
      "name": "chore/*",
      "prerelease": "chore"
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/npm"
  ]
}
